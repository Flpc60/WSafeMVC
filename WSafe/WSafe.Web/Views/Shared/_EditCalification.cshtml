<div class="container" style="background-color:mediumaquamarine">

    <div class="row">
        <div class="col-md-10">
            <div class="form-group" style="text-align:center; font-size:medium">
                <p id="txtStandard"></p>
            </div>
        </div>
    </div>
    <div class="col-md-2"></div>
    <div class="row">
        <div class="col-md-10">
            <div class="form-check">
                <input class="form-check-input" type="radio" name="txtCumple" id="txtCumple" />
                <label class="form-check-label" for="txtCumple"> Cumple totalmente </label>
                <input class="form-check-input" type="radio" name="txtNoCumple" id="txtNoCumple" checked />
                <label class="form-check-label" for="txtNoCumple"> No Cumple totalmente </label>
            </div>
            <div class="form-check">
            </div>
        </div>
        <div class="col-md-2"></div>
    </div>

    <div class="row">
        <div class="col-md-10">
            <div class="form-check">
                <input class="form-check-input" type="radio" name="txtJustify" id="txtJustify" />
                <label class="form-check-label" for="txtJustify"> No aplica Justifica </label>
            </div>
            <div class="form-check">
                <input class="form-check-input" type="radio" name="txtNoJustify" id="txtNoJustify" checked />
                <label class="form-check-label" for="txtNoJustify"> No aplica No Justifica </label>
            </div>
        </div>
        <div class="col-md-2"></div>
    </div>

    <div class="row">
        <div class="col-md-10">
            <div class="form-group">
                <div class="col-md-6">
                    <hr />
                    <label for="txtObservation">OBSERVACIONES :</label>
                    <textarea id="txtObservation" style="resize:none;" class="form-control" rows=2 cols=70 required></textarea>
                </div>
            </div>
        </div>
        <div class="col-md-2"></div>
    </div>

    <div class="row">
        <div class="col-md-10">
            <div class="form-group">
                <p id="txtCalificacion"></p>
            </div>
        </div>
    </div>
    <div class="col-md-2"></div>
</div>