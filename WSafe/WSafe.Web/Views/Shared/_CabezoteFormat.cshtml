@{
    var logo = @Session["logo"].ToString();
}

<div class="container" style="background-color: lightgray">
    <div class="row align-items-center">
        <div class="col-md-12 text-center" style="display:inline-flex;">
            <img src="@Server.MapPath(@logo)" style="width:100px; height:100px" alt="Logo">
            <div><strong><b>@Session["organization"].ToString()</b></strong></div>
        </div>
    </div>
    <div class="row" style="text-align:center;"><strong><b>@ViewBag.titulo AÑO: @Session["year"].ToString()</b></strong></div>
    <div class="row" style="text-align:center;">ESTANDAR: <strong><b>@ViewBag.estandar FECHA REPORTE: @ViewBag.fecha</b></strong></div>
    <div class="row" style="text-align:center;"></div>
    <div class="row" style="text-align:center;"></div>
</div>
